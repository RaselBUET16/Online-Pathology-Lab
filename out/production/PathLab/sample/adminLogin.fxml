<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.adminLogin">
    <children>
        <Label layoutX="194.0" layoutY="182.0" prefHeight="26.0" prefWidth="90.0" text="Username">
            <font>
                <Font size="17.0" />
            </font>
        </Label>
        <Label layoutX="194.0" layoutY="243.0" text="Password">
            <font>
                <Font size="17.0" />
            </font>
        </Label>
        <Label fx:id="passDontMatch" alignment="CENTER" layoutX="156.0" layoutY="326.0" prefHeight="26.0" prefWidth="487.0" textFill="#d50e0e">
            <font>
                <Font size="17.0" />
            </font>
        </Label>
        <TextField fx:id="username" alignment="CENTER" layoutX="321.0" layoutY="182.0" prefHeight="26.0" prefWidth="190.0" promptText="username" />
