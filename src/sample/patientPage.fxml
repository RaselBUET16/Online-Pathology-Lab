<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.patientPageController">
   <children>
      <ComboBox fx:id="organComboBox" layoutX="269.0" layoutY="115.0" prefHeight="26.0" prefWidth="262.0" promptText="                                Organ" />
      <ComboBox fx:id="diseaseComboBox" layoutX="269.0" layoutY="172.0" prefHeight="26.0" prefWidth="254.0" promptText="                                Disease" />
      <Label layoutX="106.0" layoutY="65.0" text="Search Test">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="searchedText" alignment="CENTER" layoutX="269.0" layoutY="65.0" prefHeight="26.0" prefWidth="262.0" promptText="Test Name" />
      <Button layoutX="614.0" layoutY="116.0" mnemonicParsing="false" onAction="#organSearchOnAction" prefHeight="26.0" prefWidth="85.0" text="Search" />
      <Button layoutX="614.0" layoutY="173.0" mnemonicParsing="false" onAction="#diseaseSearchOnAction" prefHeight="26.0" prefWidth="86.0" text="Search" />
      <Button layoutX="614.0" layoutY="65.0" mnemonicParsing="false" onAction="#testSearchOnAction" prefHeight="26.0" prefWidth="85.0" text="Search" />
      <Label layoutX="106.0" layoutY="116.0" text="Search By Organ">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label layoutX="106.0" layoutY="173.0" text="Search By Disease">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button layoutX="329.0" layoutY="292.0" mnemonicParsing="false" onAction="#selectedTestOnAction" prefHeight="26.0" prefWidth="142.0" text="Selected Tests">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="329.0" layoutY="342.0" mnemonicParsing="false" onAction="#submittedTestOnAction" prefHeight="30.0" prefWidth="142.0" text="Submitted Tests">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="329.0" layoutY="395.0" mnemonicParsing="false" onAction="#generatedReportOnAction" prefHeight="30.0" prefWidth="141.0" text="Generated Reports">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="361.0" layoutY="482.0" mnemonicParsing="false" onAction="#logoutOnAction" prefHeight="26.0" prefWidth="70.0" text="LogOut" />
   </children>
</AnchorPane>
